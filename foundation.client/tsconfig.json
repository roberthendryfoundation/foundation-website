{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [ "ES2020", "DOM", "DOM.Iterable" ],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "types": [ "vite/client" ],
    "baseUrl": "."
  },
  "include": [ "src", "vite-env.d.ts" ]
}